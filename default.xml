<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- defaults for entries that haven't specified all required fields-->
    <default revision="thud" />

    <!-- remotes -->
    <remote name="github" fetch="ssh://git@github.com/" />
    <remote name="yocto" fetch="git://git.yoctoproject.org" />
    <remote name="oe" fetch="git://git.openembedded.org" />

    <!-- build scripts -->
    <project name="armmbed/wigwag-build-env"
        path="build-env"
        revision="master"
        remote="github" />

    <!-- yocto core -->
    <project name="poky"
        path="poky"
        remote="yocto" />

    <project name="meta-openembedded"
        path="poky/meta-openembedded"
        remote="oe" />

    <project name="meta-virtualization"
        path="poky/meta-virtualization"
        remote="yocto" />

    <project name="meta-security"
        path="poky/meta-security"
        remote="yocto" />

    <!-- bsps -->
    <project name="meta-raspberrypi"
        path="poky/meta-raspberrypi"
        remote="yocto" />

    <!-- distros -->

    <!-- software -->
    <project name="armmbed/meta-gateway-ww"
        path="poky/meta-gateway-ww"
        revision="master"
        remote="github" />

    <project name="aaronovz1/meta-nodejs"
        path="poky/meta-nodejs"
        revision="pyro"
        remote="github" />

</manifest>
